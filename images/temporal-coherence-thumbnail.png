<svg width="400" height="200" viewBox="0 0 400 200" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="200" fill="#e8f5e8"/>
  <rect x="20" y="20" width="360" height="160" fill="white" stroke="#333" stroke-width="2"/>
  
  <!-- Temporal Coherence Visualization -->
  <rect x="50" y="60" width="40" height="30" fill="#4caf50" opacity="0.8"/>
  <rect x="100" y="60" width="40" height="30" fill="#4caf50" opacity="0.8"/>
  <rect x="150" y="60" width="40" height="30" fill="#4caf50" opacity="0.8"/>
  <rect x="200" y="60" width="40" height="30" fill="#4caf50" opacity="0.8"/>
  <rect x="250" y="60" width="40" height="30" fill="#4caf50" opacity="0.8"/>
  <rect x="300" y="60" width="40" height="30" fill="#4caf50" opacity="0.8"/>
  
  <!-- Frame Numbers -->
  <text x="70" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Frame 1</text>
  <text x="120" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Frame 2</text>
  <text x="170" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Frame 3</text>
  <text x="220" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Frame 4</text>
  <text x="270" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Frame 5</text>
  <text x="320" y="105" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Frame 6</text>
  
  <!-- Consistency Lines -->
  <path d="M70 45 L120 45" stroke="#4caf50" stroke-width="2"/>
  <path d="M120 45 L170 45" stroke="#4caf50" stroke-width="2"/>
  <path d="M170 45 L220 45" stroke="#4caf50" stroke-width="2"/>
  <path d="M220 45 L270 45" stroke="#4caf50" stroke-width="2"/>
  <path d="M270 45 L320 45" stroke="#4caf50" stroke-width="2"/>
  
  <!-- Title -->
  <text x="200" y="130" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#333">Temporal Coherence</text>
  <text x="200" y="150" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">Frame Consistency</text>
</svg>
