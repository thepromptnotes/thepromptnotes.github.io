<svg width="400" height="200" viewBox="0 0 400 200" fill="none" xmlns="http://www.w3.org/2000/svg">
  <rect width="400" height="200" fill="#fff3e0"/>
  <rect x="20" y="20" width="360" height="160" fill="white" stroke="#333" stroke-width="2"/>
  
  <!-- Computer Vision Pipeline -->
  <rect x="50" y="60" width="60" height="40" fill="#ff9800" opacity="0.8"/>
  <rect x="130" y="60" width="60" height="40" fill="#ff9800" opacity="0.6"/>
  <rect x="210" y="60" width="60" height="40" fill="#ff9800" opacity="0.4"/>
  <rect x="290" y="60" width="60" height="40" fill="#ff9800" opacity="0.2"/>
  
  <!-- Pipeline Flow -->
  <path d="M110 80 L130 80" stroke="#333" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M190 80 L210 80" stroke="#333" stroke-width="2" marker-end="url(#arrowhead)"/>
  <path d="M270 80 L290 80" stroke="#333" stroke-width="2" marker-end="url(#arrowhead)"/>
  
  <!-- Labels -->
  <text x="80" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Input</text>
  <text x="160" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Process</text>
  <text x="240" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Extract</text>
  <text x="320" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="10" fill="#333">Classify</text>
  
  <!-- Title -->
  <text x="200" y="140" text-anchor="middle" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="#333">Computer Vision</text>
  <text x="200" y="160" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" fill="#666">AI Processing</text>
  
  <defs>
    <marker id="arrowhead" markerWidth="10" markerHeight="7" refX="9" refY="3.5" orient="auto">
      <polygon points="0 0, 10 3.5, 0 7" fill="#333"/>
    </marker>
  </defs>
</svg>
